package sec03.exam06;

public class StringConcatExample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int value = 10+2+8;
		System.out.println(value);
		
		String str1 = 10 + 2 + "8";
		System.out.println(str1);
		
		String str2 = 10 + "2" + 8;
		System.out.println(str2);
		
	}

}
